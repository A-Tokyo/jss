{
  "presets": [
    "stage-0",
    "es2015"
  ],
  "plugins": [
    "transform-es3-member-expression-literals",
    "transform-es3-property-literals",
    "transform-flow-strip-types",
    "transform-class-properties",
    "inline-version"
  ],
  "env": {
    "test": {
      "plugins": [
        "rewire"
      ]
    }
  }
}
